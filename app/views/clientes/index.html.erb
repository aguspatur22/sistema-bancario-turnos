<p style="color: green"><%= notice %></p>

<h1>Clientes</h1>

<div id="clientes">
  <% @clientes.each do |cliente| %>
    <%= render cliente %>
    <p>
      <%= link_to "Show this cliente", cliente %>
    </p>
  <% end %>
</div>

<% if can? :create, Cliente %>
  <%= link_to "New cliente", new_cliente_path %>
<% end %>
